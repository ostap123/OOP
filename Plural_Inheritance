class Employee{  
 float salary=400;  
}  
class Programmer extends Employee{  
 int bonus=10000;  

}
class BusDriver extends Employee{
 int reward= 3000;
  public static void main(String args[]){  
   Programmer p=new Programmer();
   BusDriver b=new BusDriver();
   System.out.println("Programmer salary is:"+ p.salary + p.bonus);  
   System.out.println("Busdriver salary is:"+ b.salary + b.reward);
}  
}
